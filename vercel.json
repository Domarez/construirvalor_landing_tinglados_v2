{
  "version": 2,
  "builds": [
    {
      "src": "vite.config.js",
      "use": "@vercel/vite",
      "config": {
        "outputDirectory": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/reformas/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico|json))",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "dest": "/$1"
    },
    {
      "src": "/reformas/?(.*)",
      "dest": "/"
    }
  ]
}
